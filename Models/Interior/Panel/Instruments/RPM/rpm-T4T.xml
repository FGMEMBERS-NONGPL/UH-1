<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>rpm.ac</path>

  <animation>
    <object-name>fond</object-name>
    <object-name>face</object-name>
    <object-name>Lneedle</object-name>
    <object-name>Rneedle</object-name>
    <object-name>visses</object-name>

    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>Aircraft/Generic/Effects/glass_shader.png</texture>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>Lneedle</object-name>
    <object-name>Rneedle</object-name>
    <emission>
      <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
      <red>   0.80 </red>
      <green> 0.20 </green>
      <blue>  0.00 </blue>
    </emission>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Lneedle</object-name>
    <property>/fdm/jsbsim/propulsion/engine[0]/propeller-rpm</property>
    <interpolation>
      <entry><ind>0</ind><dep>-260</dep></entry>
      <entry><ind>357</ind><dep>70</dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Rneedle</object-name>
    <property>/fdm/jsbsim/propulsion/engine[0]/n1</property>
    <interpolation>
      <entry><ind>0</ind><dep>-260</dep></entry>
      <entry><ind>100</ind><dep>60</dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>

